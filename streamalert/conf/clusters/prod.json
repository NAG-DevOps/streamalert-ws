{
  "id": "prod",
  "modules": {
    "cloudtrail": {
      "enable_kinesis": false,
      "enable_logging": true
    },
    "cloudwatch_monitoring": {
      "enabled": true,
      "kinesis_alarms_enabled": true,
      "lambda_alarms_enabled": true
    },
    "kinesis": {
      "streams": {
        "create_user": true,
        "retention": 24,
        "shards": 1,
        "trusted_accounts": []
      }
    },
    "kinesis_events": {
      "batch_size": 100,
      "enabled": true
    },
    "stream_alert": {
      "alert_processor": {
        "current_version": 3,
        "log_level": "info",
        "memory": 128,
        "timeout": 60
      },
      "rule_processor": {
        "current_version": 3,
        "enable_metrics": true,
        "log_level": "info",
        "memory": 128,
        "timeout": 10
      }
    }
  },
  "outputs": {
    "kinesis": [
      "username",
      "access_key_id",
      "secret_key"
    ]
  },
  "region": "us-east-1"
}